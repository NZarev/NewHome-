import java.util.Scanner;

public class NewHome {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        String flowerType = scanner.nextLine();
        int numberOfFlowers = scanner.nextInt();
        double budget = scanner.nextDouble();

        double pricePerFlower = 0.0;
        double discount = 0.0;
        double surcharge = 0.0;

        switch (flowerType) {
            case "Roses":
                pricePerFlower = 5.00;
                if (numberOfFlowers > 80) {
                    discount = 0.10;
                }
                break;
            case "Dahlias":
                pricePerFlower = 3.80;
                if (numberOfFlowers > 90) {
                    discount = 0.15;
                }
                break;
            case "Tulips":
                pricePerFlower = 2.80;
                if (numberOfFlowers > 80) {
                    discount = 0.15;
                }
                break;
            case "Narcissus":
                pricePerFlower = 3.00;
                if (numberOfFlowers < 120) {
                    surcharge = 0.15;
                }
                break;
            case "Gladiolus":
                pricePerFlower = 2.50;
                if (numberOfFlowers < 80) {
                    surcharge = 0.20;
                }
                break;
            default:
                System.out.println("Invalid flower type.");
                return;
        }

        double totalPrice = numberOfFlowers * pricePerFlower;
        totalPrice -= totalPrice * discount;
        totalPrice += totalPrice * surcharge;

        if (budget >= totalPrice) {
            double remainingBudget = budget - totalPrice;
            System.out.printf("Hey, you have a great garden with %d %s and %.2f leva left.%n", numberOfFlowers, flowerType, remainingBudget);
        } else {
            double neededMoney = totalPrice - budget;
            System.out.printf("Not enough money, you need %.2f leva more.%n", neededMoney);
        }

        scanner.close();
    }
}
